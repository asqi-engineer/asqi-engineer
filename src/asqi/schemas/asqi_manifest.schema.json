{
  "$defs": {
    "AudioFeature": {
      "description": "Corresponds to HuggingFace's Audio feature type.",
      "properties": {
        "feature_type": {
          "const": "Audio",
          "default": "Audio",
          "description": "Feature type discriminator",
          "title": "Feature Type",
          "type": "string"
        },
        "name": {
          "description": "The name of the feature",
          "title": "Name",
          "type": "string"
        },
        "required": {
          "default": false,
          "description": "Whether this feature is required in the dataset. If False, the feature may be absent or contain null values.",
          "title": "Required",
          "type": "boolean"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Description of the audio feature",
          "title": "Description"
        }
      },
      "required": [
        "name"
      ],
      "title": "AudioFeature",
      "type": "object"
    },
    "ClassLabelFeature": {
      "description": "Corresponds to HuggingFace's ClassLabel feature type. Represents categorical data with named categories.",
      "properties": {
        "feature_type": {
          "const": "ClassLabel",
          "default": "ClassLabel",
          "description": "Feature type discriminator",
          "title": "Feature Type",
          "type": "string"
        },
        "name": {
          "description": "The name of the feature",
          "title": "Name",
          "type": "string"
        },
        "names": {
          "description": "Category names (e.g., ['positive', 'negative', 'neutral'])",
          "items": {
            "type": "string"
          },
          "minItems": 1,
          "title": "Names",
          "type": "array"
        },
        "required": {
          "default": false,
          "description": "Whether this feature is required in the dataset. If False, the feature may be absent or contain null values.",
          "title": "Required",
          "type": "boolean"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Description of the classification categories",
          "title": "Description"
        }
      },
      "required": [
        "name",
        "names"
      ],
      "title": "ClassLabelFeature",
      "type": "object"
    },
    "DatasetFeature": {
      "description": "Defines a feature/column within a dataset.\n\nThe dtype field uses HuggingFace datasets dtype values.\nCommon types: 'string', 'int64', 'float32', 'bool'.",
      "properties": {
        "name": {
          "description": "The name of the feature.",
          "title": "Name",
          "type": "string"
        },
        "dtype": {
          "description": "The data type of the feature. Common types: 'string', 'int64', 'int32', 'float64', 'float32', 'bool'. See: https://huggingface.co/docs/datasets/about_dataset_features",
          "enum": [
            "null",
            "bool",
            "int8",
            "int16",
            "int32",
            "int64",
            "uint8",
            "uint16",
            "uint32",
            "uint64",
            "float16",
            "float32",
            "float",
            "float64",
            "double",
            "time32[s]",
            "time32[ms]",
            "time64[us]",
            "time64[ns]",
            "timestamp[s]",
            "timestamp[ms]",
            "timestamp[us]",
            "timestamp[ns]",
            "date32",
            "date64",
            "duration[s]",
            "duration[ms]",
            "duration[us]",
            "duration[ns]",
            "binary",
            "large_binary",
            "binary_view",
            "string",
            "large_string",
            "string_view"
          ],
          "title": "Dtype",
          "type": "string"
        },
        "required": {
          "default": false,
          "description": "Whether this feature is required in the dataset. If False, the feature may be absent or contain null values.",
          "title": "Required",
          "type": "boolean"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Description of the feature - data type, purpose etc.",
          "title": "Description"
        }
      },
      "required": [
        "name",
        "dtype"
      ],
      "title": "DatasetFeature",
      "type": "object"
    },
    "DatasetType": {
      "description": "Supported dataset types for InputDataset.",
      "enum": [
        "huggingface",
        "pdf",
        "txt"
      ],
      "title": "DatasetType",
      "type": "string"
    },
    "DictFeature": {
      "description": "Corresponds to HuggingFace's \"Python dict\" feature type.",
      "properties": {
        "feature_type": {
          "const": "Dict",
          "default": "Dict",
          "description": "Feature type discriminator",
          "title": "Feature Type",
          "type": "string"
        },
        "name": {
          "description": "The name of the feature",
          "title": "Name",
          "type": "string"
        },
        "fields": {
          "description": "Named fields within the dict. Each field can be any HFFeature type (Value, List, Dict, etc.)",
          "items": {
            "discriminator": {
              "mapping": {
                "Audio": "#/$defs/AudioFeature",
                "ClassLabel": "#/$defs/ClassLabelFeature",
                "Dict": "#/$defs/DictFeature",
                "Image": "#/$defs/ImageFeature",
                "List": "#/$defs/ListFeature",
                "Value": "#/$defs/ValueFeature",
                "Video": "#/$defs/VideoFeature"
              },
              "propertyName": "feature_type"
            },
            "oneOf": [
              {
                "$ref": "#/$defs/ValueFeature"
              },
              {
                "$ref": "#/$defs/ListFeature"
              },
              {
                "$ref": "#/$defs/DictFeature"
              },
              {
                "$ref": "#/$defs/ClassLabelFeature"
              },
              {
                "$ref": "#/$defs/ImageFeature"
              },
              {
                "$ref": "#/$defs/AudioFeature"
              },
              {
                "$ref": "#/$defs/VideoFeature"
              }
            ]
          },
          "minItems": 1,
          "title": "Fields",
          "type": "array"
        },
        "required": {
          "default": false,
          "description": "Whether this feature is required in the dataset. If False, the feature may be absent or contain null values.",
          "title": "Required",
          "type": "boolean"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Description of the dict feature",
          "title": "Description"
        }
      },
      "required": [
        "name",
        "fields"
      ],
      "title": "DictFeature",
      "type": "object"
    },
    "EnvironmentVariable": {
      "description": "Defines an environment variable required by the test container.",
      "properties": {
        "name": {
          "description": "Environment variable name (e.g., 'OPENAI_API_KEY', 'HF_TOKEN').",
          "title": "Name",
          "type": "string"
        },
        "required": {
          "default": true,
          "description": "Whether this environment variable is mandatory for execution.",
          "title": "Required",
          "type": "boolean"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Explanation of what this variable is used for.",
          "title": "Description"
        }
      },
      "required": [
        "name"
      ],
      "title": "EnvironmentVariable",
      "type": "object"
    },
    "ImageFeature": {
      "description": "Corresponds to HuggingFace's Image feature type.",
      "properties": {
        "feature_type": {
          "const": "Image",
          "default": "Image",
          "description": "Feature type discriminator",
          "title": "Feature Type",
          "type": "string"
        },
        "name": {
          "description": "The name of the feature",
          "title": "Name",
          "type": "string"
        },
        "required": {
          "default": false,
          "description": "Whether this feature is required in the dataset. If False, the feature may be absent or contain null values.",
          "title": "Required",
          "type": "boolean"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Description of the image feature",
          "title": "Description"
        }
      },
      "required": [
        "name"
      ],
      "title": "ImageFeature",
      "type": "object"
    },
    "InputDataset": {
      "description": "Defines a dataset input that the container requires.\n\nSupported dataset types:\n- 'huggingface': HuggingFace datasets (requires features to be defined)\n- 'pdf': PDF documents\n- 'txt': Plain text files\n\nSupports \"either/or\" relationships by accepting multiple types:\n- type: \"pdf\" - Single type\n- type: [\"pdf\", \"txt\"] - Either PDF or TXT\n- type: [\"huggingface\", \"pdf\", \"txt\"] - Any of these formats",
      "properties": {
        "name": {
          "description": "The dataset name, e.g., 'evaluation_data', 'test_prompts'.",
          "title": "Name",
          "type": "string"
        },
        "required": {
          "default": true,
          "description": "Whether this dataset is mandatory for execution.",
          "title": "Required",
          "type": "boolean"
        },
        "type": {
          "anyOf": [
            {
              "$ref": "#/$defs/DatasetType"
            },
            {
              "items": {
                "$ref": "#/$defs/DatasetType"
              },
              "type": "array"
            }
          ],
          "description": "The dataset type(s): single type or list of accepted types. Examples: 'huggingface', ['pdf', 'txt'], or ['huggingface', 'pdf', 'txt'].",
          "title": "Type"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Description of the dataset's role in the test.",
          "title": "Description"
        },
        "features": {
          "anyOf": [
            {
              "items": {
                "anyOf": [
                  {
                    "$ref": "#/$defs/DatasetFeature"
                  },
                  {
                    "discriminator": {
                      "mapping": {
                        "Audio": "#/$defs/AudioFeature",
                        "ClassLabel": "#/$defs/ClassLabelFeature",
                        "Dict": "#/$defs/DictFeature",
                        "Image": "#/$defs/ImageFeature",
                        "List": "#/$defs/ListFeature",
                        "Value": "#/$defs/ValueFeature",
                        "Video": "#/$defs/VideoFeature"
                      },
                      "propertyName": "feature_type"
                    },
                    "oneOf": [
                      {
                        "$ref": "#/$defs/ValueFeature"
                      },
                      {
                        "$ref": "#/$defs/ListFeature"
                      },
                      {
                        "$ref": "#/$defs/DictFeature"
                      },
                      {
                        "$ref": "#/$defs/ClassLabelFeature"
                      },
                      {
                        "$ref": "#/$defs/ImageFeature"
                      },
                      {
                        "$ref": "#/$defs/AudioFeature"
                      },
                      {
                        "$ref": "#/$defs/VideoFeature"
                      }
                    ]
                  }
                ]
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "List of required features within a HuggingFace dataset. Supports both simple scalar features and complex feature types.",
          "title": "Features"
        }
      },
      "required": [
        "name",
        "type"
      ],
      "title": "InputDataset",
      "type": "object"
    },
    "InputParameter": {
      "description": "Defines a parameter that can be passed to the test container.\nSupports simple types (string, integer, float, boolean) as well as list, object, enum.",
      "properties": {
        "name": {
          "description": "Parameter name",
          "title": "Name",
          "type": "string"
        },
        "type": {
          "description": "Parameter type",
          "enum": [
            "string",
            "integer",
            "float",
            "boolean",
            "list",
            "object",
            "enum"
          ],
          "title": "Type",
          "type": "string"
        },
        "required": {
          "default": false,
          "description": "Whether this parameter is required",
          "title": "Required",
          "type": "boolean"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Human-readable description of the parameter",
          "title": "Description"
        },
        "items": {
          "anyOf": [
            {
              "enum": [
                "string",
                "integer",
                "float",
                "boolean",
                "object",
                "enum"
              ],
              "type": "string"
            },
            {
              "$ref": "#/$defs/InputParameter"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "For type='list': defines the schema for list items. Can be a simple type name string (e.g., 'string', 'integer') for basic typed lists, or a full InputParameter object for complex items (enums, objects, nested lists). ",
          "title": "Items"
        },
        "properties": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/InputParameter"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "For type='object': list of nested parameter definitions. Each property is a full InputParameter with its own name, type, and constraints. ",
          "title": "Properties"
        },
        "choices": {
          "anyOf": [
            {
              "items": {
                "anyOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "integer"
                  },
                  {
                    "type": "number"
                  }
                ]
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "For type='enum': list of allowed values. Required when type='enum'.",
          "title": "Choices"
        },
        "default": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "integer"
            },
            {
              "type": "number"
            },
            {
              "type": "boolean"
            },
            {
              "items": {},
              "type": "array"
            },
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Default value for this parameter when not provided by user",
          "title": "Default"
        }
      },
      "required": [
        "name",
        "type"
      ],
      "title": "InputParameter",
      "type": "object"
    },
    "ListFeature": {
      "description": "Corresponds to HuggingFace's List/Sequence feature type.",
      "properties": {
        "feature_type": {
          "const": "List",
          "default": "List",
          "description": "Feature type discriminator",
          "title": "Feature Type",
          "type": "string"
        },
        "name": {
          "description": "The name of the feature",
          "title": "Name",
          "type": "string"
        },
        "feature": {
          "anyOf": [
            {
              "enum": [
                "null",
                "bool",
                "int8",
                "int16",
                "int32",
                "int64",
                "uint8",
                "uint16",
                "uint32",
                "uint64",
                "float16",
                "float32",
                "float",
                "float64",
                "double",
                "time32[s]",
                "time32[ms]",
                "time64[us]",
                "time64[ns]",
                "timestamp[s]",
                "timestamp[ms]",
                "timestamp[us]",
                "timestamp[ns]",
                "date32",
                "date64",
                "duration[s]",
                "duration[ms]",
                "duration[us]",
                "duration[ns]",
                "binary",
                "large_binary",
                "binary_view",
                "string",
                "large_string",
                "string_view"
              ],
              "type": "string"
            },
            {
              "enum": [
                "Image",
                "Audio",
                "Video",
                "ClassLabel",
                "Dict",
                "List"
              ],
              "type": "string"
            }
          ],
          "description": "List element type. Can be: (1) scalar dtype string ('string', 'int32', etc.) for List(Value(dtype)), or (2) simple feature type name ('Image', 'Audio', 'Video', 'ClassLabel', 'Dict', 'List') for List(FeatureType()). For complex nested structures with custom parameters, define nested ListFeature or DictFeature objects.",
          "title": "Feature"
        },
        "length": {
          "default": -1,
          "description": "List length constraint: -1 for variable-length lists, >=0 for fixed-length sequences.",
          "title": "Length",
          "type": "integer"
        },
        "required": {
          "default": false,
          "description": "Whether this feature is required in the dataset. If False, the feature may be absent or contain null values.",
          "title": "Required",
          "type": "boolean"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Description of the list feature",
          "title": "Description"
        }
      },
      "required": [
        "name",
        "feature"
      ],
      "title": "ListFeature",
      "type": "object"
    },
    "OutputArtifact": {
      "description": "Defines a file artifact generated by the test container.",
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "path": {
          "title": "Path",
          "type": "string"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Description"
        }
      },
      "required": [
        "name",
        "path"
      ],
      "title": "OutputArtifact",
      "type": "object"
    },
    "OutputDataset": {
      "description": "Defines a dataset output that will be generated by the test container.",
      "properties": {
        "name": {
          "description": "The name of this output dataset (e.g., 'augmented_rag_data')",
          "title": "Name",
          "type": "string"
        },
        "type": {
          "$ref": "#/$defs/DatasetType",
          "description": "Type of dataset: huggingface, pdf, or txt"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Description of the output dataset's purpose and contents",
          "title": "Description"
        },
        "features": {
          "anyOf": [
            {
              "items": {
                "anyOf": [
                  {
                    "$ref": "#/$defs/DatasetFeature"
                  },
                  {
                    "discriminator": {
                      "mapping": {
                        "Audio": "#/$defs/AudioFeature",
                        "ClassLabel": "#/$defs/ClassLabelFeature",
                        "Dict": "#/$defs/DictFeature",
                        "Image": "#/$defs/ImageFeature",
                        "List": "#/$defs/ListFeature",
                        "Value": "#/$defs/ValueFeature",
                        "Video": "#/$defs/VideoFeature"
                      },
                      "propertyName": "feature_type"
                    },
                    "oneOf": [
                      {
                        "$ref": "#/$defs/ValueFeature"
                      },
                      {
                        "$ref": "#/$defs/ListFeature"
                      },
                      {
                        "$ref": "#/$defs/DictFeature"
                      },
                      {
                        "$ref": "#/$defs/ClassLabelFeature"
                      },
                      {
                        "$ref": "#/$defs/ImageFeature"
                      },
                      {
                        "$ref": "#/$defs/AudioFeature"
                      },
                      {
                        "$ref": "#/$defs/VideoFeature"
                      }
                    ]
                  }
                ]
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "List of required features within a HuggingFace dataset. Supports both simple scalar features and complex feature types.",
          "title": "Features"
        }
      },
      "required": [
        "name",
        "type"
      ],
      "title": "OutputDataset",
      "type": "object"
    },
    "OutputMetric": {
      "description": "Defines a metric that will be present in the test container's output.",
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "type": {
          "enum": [
            "string",
            "integer",
            "float",
            "boolean",
            "list",
            "object"
          ],
          "title": "Type",
          "type": "string"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Description"
        }
      },
      "required": [
        "name",
        "type"
      ],
      "title": "OutputMetric",
      "type": "object"
    },
    "OutputReports": {
      "description": "Defines a report that will be generated by the test container.",
      "properties": {
        "name": {
          "description": "The name of the report ('detailed_report', 'summary_report', ...).",
          "title": "Name",
          "type": "string"
        },
        "type": {
          "description": "The report file format ('pdf' or 'html').",
          "enum": [
            "pdf",
            "html"
          ],
          "title": "Type",
          "type": "string"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Short description of the report content.",
          "title": "Description"
        }
      },
      "required": [
        "name",
        "type"
      ],
      "title": "OutputReports",
      "type": "object"
    },
    "SystemInput": {
      "description": "Defines a system input that the container requires.",
      "properties": {
        "name": {
          "description": "The system input name, e.g., 'system_under_test', 'simulator_system', 'evaluator_system'.",
          "title": "Name",
          "type": "string"
        },
        "type": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          ],
          "description": "The system type(s) accepted. Can be a single string (e.g., 'llm_api') or a list of strings (e.g., ['llm_api', 'vlm_api']) for containers that support multiple system types. Valid types: 'llm_api', 'rest_api', 'rag_api', 'image_generation_api', 'image_editing_api', 'vlm_api', 'agent_cli'.",
          "title": "Type"
        },
        "required": {
          "default": true,
          "description": "Whether this system input is required.",
          "title": "Required",
          "type": "boolean"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Description of the system's role in the test.",
          "title": "Description"
        }
      },
      "required": [
        "name",
        "type"
      ],
      "title": "SystemInput",
      "type": "object"
    },
    "ValueFeature": {
      "description": "Corresponds to HuggingFace's Value feature type.\nRepresents a scalar value feature (string, int64, float32, bool, etc.)",
      "properties": {
        "feature_type": {
          "const": "Value",
          "default": "Value",
          "description": "Feature type discriminator",
          "title": "Feature Type",
          "type": "string"
        },
        "name": {
          "description": "The name of the feature",
          "title": "Name",
          "type": "string"
        },
        "dtype": {
          "description": "The data type of the feature. Common types: 'string', 'int64', 'int32', 'float64', 'float32', 'bool'. See: https://huggingface.co/docs/datasets/about_dataset_features",
          "enum": [
            "null",
            "bool",
            "int8",
            "int16",
            "int32",
            "int64",
            "uint8",
            "uint16",
            "uint32",
